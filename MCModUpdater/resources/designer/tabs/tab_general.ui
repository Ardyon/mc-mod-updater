   <property name="styleSheet">
    <string notr="true">#mods {
	background-color: #2a2a2a;
	border-radius: 15px;
}


/* Label */
QFrame QLabel {
	color: #DCE4EE;
}


/* Frame */
QFrame,
#foundModsContainer {
	background-color: #2a2a2a;
}


/* modWidget */
#foundModsContainer &gt; QWidget {
	background-color: rgba(255, 255, 255, 0.1);
	border-radius: 10px;
}


/* modWidget Label */
#foundModsContainer &gt; QWidget QLabel {
	background-color: rgba(255, 255, 255, 0.0);
}


/* modWidget PushButton */
#foundModsContainer &gt; QWidget QPushButton {
	background-color: #9e2c24;
	border-radius: 5px;
}
#foundModsContainer &gt; QWidget QPushButton:pressed {
	background-color: #6b1d18;
}


/* ScrollArea */
QScrollArea {
	background-color: #2a2a2a;
	border-radius: 15px;
}
/* VERTICAL SCROLLBAR */
 QScrollBar:vertical {
	border: none;
    background: #2a2a2a;
    width: 10px;
 }
/*  HANDLE BAR VERTICAL */
QScrollBar::handle:vertical {	
	background: #DCE4EE;
	border-radius: 5px;
}
QScrollBar::handle:vertical:pressed {	
	background-color: #acb3bb;
}
/* BTN TOP - SCROLLBAR */
QScrollBar::sub-line:vertical, 
QScrollBar::add-line:vertical {
	height: 0px;
}
/* RESET ARROW */
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical,
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}
/* HORIZONTAL SCROLLBAR */
QScrollBar:horizontal {
	border: none;
    background: #3f3f3f;
    height: 10px;
	border-radius: 0px;
}
/*  HANDLE BAR HORIZONTAL */
QScrollBar::handle:horizontal {
	background: #DCE4EE;
	border-radius: 5px;
}
QScrollBar::handle:horizontal:pressed {	
	background-color: #acb3bb;
}
/* BTN TOP - SCROLLBAR */
QScrollBar::sub-line:horizontal, 
QScrollBar::add-line:horizontal {
	width: 0px;
}
/* RESET ARROW */
QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal,
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
	background: none;
}</string>
