   <property name="styleSheet">
    <string notr="true">#settings {
	background-color: #2a2a2a;
	border-radius: 15px;
}


/* Label */
QLabel {
	color: #DCE4EE
}


/* PushButton */
QPushButton {
	color: #DCE4EE;
	background-color: #2f5ea1;
	border-radius: 8px;
}
QPushButton:pressed {
	color: #DCE4EE;
	background-color: #20406d;
}
QPushButton:disabled {
	color: #959aa1;
	background-color: #183154;
}
#stopButton {
	background-color: #9e2c24;
}
#stopButton:pressed {
	background-color: #6b1d18;
}


/* LineEdit */
QLineEdit {
	color: #DCE4EE;
	background-color: rgba(255, 255, 255, 0.1);
	border-radius: 3px;
	border: 1px solid #DCE4EE;
}
QLineEdit:disabled {
	color: #959aa1;
	background-color: rgba(255, 255, 255, 0.05);
	border: 1px solid #959aa1;
}


/* ComboBox */
QComboBox {
	padding-left: 10px;
	border-radius: 0px;
	color: #DCE4EE;
	background-color: rgba(255, 255, 255, 0.1);
}
QComboBox:disabled {
	color: #959aa1;
	background-color: rgba(255, 255, 255, 0.05);
}
QComboBox QListView {
	padding: 5px;
	color: #DCE4EE;
/*	background-color: rgba(0, 0, 0, 1); */
	background-color: #3f3f3f;
	border-radius: 0px;
	outline: 0px;
}
QComboBox QListView:disabled {
	color: #959aa1;
	background-color: rgba(255, 255, 255, 0.05);
}


/* CheckBox */
QCheckBox {
	color: #DCE4EE;
}
QCheckBox:disabled {
	color: #959aa1;
}


/* RadioButton */
QRadioButton {
  color: #DCE4EE;
}


/* PlainTextEdit */
QPlainTextEdit {
	padding: 3px;
	border: 1px solid #DCE4EE;
	border-radius: 10px;
	color: #DCE4EE;
	background-color: rgba(255, 255, 255, 0.1);
}
QPlainTextEdit:disabled {
	border: 1px solid #959aa1;
	color: #959aa1;
	background-color: rgba(255, 255, 255, 0.05);
}
/* SCROLLBAR */
QScrollBar {
	border: none;
    background: #3f3f3f;
	border-radius: 0px;
}
QScrollBar:vertical {
    width: 10px;
 }
QScrollBar:horizontal {
    height: 10px;
}
QScrollBar:disabled {
    background: rgba(255, 255, 255, 0.0);
 }
/* HANDLE BAR */
QScrollBar::handle {
	background: #DCE4EE;
	border-radius: 5px;
}
QScrollBar::handle:pressed {	
	background-color: #acb3bb;
}
QScrollBar::handle:disabled {
	background: #959aa1;
}
/* BTN TOP - SCROLLBAR */
QScrollBar::sub-line:vertical, 
QScrollBar::add-line:vertical {
	height: 0px;
}
/* BTN TOP - SCROLLBAR */
QScrollBar::sub-line:horizontal, 
QScrollBar::add-line:horizontal {
	width: 0px;
}
/* RESET ARROW */
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical,
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical,
QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal,
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
	background: none;
}


/* ProgressBar */
QProgressBar {
    border: 2px solid #dce4ee;
	padding: 3px;
    border-radius: 3px;
	color: #dce4ee;
    background-color: #2a2a2a;
}
QProgressBar::chunk {
	background-color: green;
}</string>
